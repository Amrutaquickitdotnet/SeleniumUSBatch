package com.java;

public class Example_003_BooleanDataType {

	// main execution point Execution
	public static void main(String[] args) {

		// boolean data type which can store value true(1) or false(0)
		boolean isJavaFun = true;//1
		// Variable is a container which hold value
		
		// dataType variable name = value
		
		boolean isFishTasty = true;
		
		System.out.println(isJavaFun);
		System.out.println(isFishTasty);
		
		int a = 10;
		int y = 9;
		System.out.println(a>y);// Return the result // True
		
		int x = 10;
		System.out.println(x==20);
		// 10==10 // true
		
		

	}
}
