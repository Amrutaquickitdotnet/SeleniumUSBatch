package com.java;
//import com.visible.*;

public class Example_017_StringusingLiteral2 {
// Array of arrays is called as multiDimensional Array
	
	
	// when you would like to save data in a tabular form like table with rows and columnns

	public static void main(String[] args) {
		
		String s1 = "Great";
		String s2 = "Great";// 
		String s3 = s1;
		
		// 6 Objects will going to be create on memory.
		// SCP => On SCP storing values belong for my string Object
		// Heap => On heap memory it is storing reference variable for string Object
		System.out.println(s1);
		System.out.println(s2);
		System.out.println(s3);
		 
		 
		 
	}
		
	}
	
	
	
	

