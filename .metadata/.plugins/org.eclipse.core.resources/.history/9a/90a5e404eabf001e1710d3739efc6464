package com.java;
//import com.visible.*;

public class Example_013_ArrayConcept {

	public static void main(String[] args) {

		int a = 9;   // 1001 
		int b = 8;// 1000
		int c = a&b; // 1001&1000 =   1000 = 8
				System.out.println(c);
				
				
				int x = 9;// 1001
				int y = 8;// 1000
				// 1001^1000 = 0001
				System.out.println("x^y="+(x^y));
	}
	//~ Not  !, !=
	
	//BitWise OR(||)
	
	
	
	
}
